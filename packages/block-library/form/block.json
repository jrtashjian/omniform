{
	"$schema": "https://raw.githubusercontent.com/WordPress/gutenberg/wp/6.3/schemas/json/block.json",
	"apiVersion": 3,
	"name": "omniform/form",
	"category": "omniform",
	"title": "OmniForm",
	"description": "Easily create professional forms for your website",
	"textdomain": "omniform",
	"attributes": {
		"ref": {
			"type": "number"
		},
		"form_title": {
			"type": "string"
		},
		"form_type": {
			"type": "string",
			"enum": [
				"standard",
				"custom"
			]
		},
		"notify_email": {
			"type": "array"
		},
		"notify_email_subject": {
			"type": "string"
		},
		"submit_action": {
			"type": "string"
		},
		"submit_method": {
			"type": "string"
		}
	},
	"supports": {
		"anchor": true,
		"align": [
			"full",
			"wide"
		],
		"html": false,
		"reusable": false,
		"__experimentalLayout": {
			"allowEditing": false
		}
	},
	"usesContext": [
		"postId",
		"postType"
	],
	"editorScript": "file:index.js",
	"editorStyle": "file:index.css",
	"style": "file:style-index.css",
	"viewScript": "file:view.js"
}