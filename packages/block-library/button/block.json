{
	"$schema": "https://raw.githubusercontent.com/WordPress/gutenberg/wp/6.2/schemas/json/block.json",
	"apiVersion": 2,
	"name": "omniform/button",
	"category": "omniform-control-simple",
	"title": "Button",
	"description": "A button for the form",
	"textdomain": "omniform",
	"attributes": {
		"buttonType": {
			"type": "string",
			"default": "button"
		},
		"buttonLabel": {
			"type": "string"
		},
		"buttonName": {
			"type": "string"
		},
		"buttonValue": {
			"type": "string"
		}
	},
	"supports": {
		"html": false,
		"color": {
			"text": true,
			"background": true
		},
		"spacing": {
			"margin": true,
			"padding": true
		},
		"__experimentalBorder": true,
		"__experimentalSelector": ".wp-block-omniform-button .wp-element-button"
	},
	"styles": [
		{ "name": "fill", "label": "Fill", "isDefault": true },
		{ "name": "outline", "label": "Outline" }
	],
	"editorScript": "file:index.js",
	"editorStyle": "file:index.css",
	"style": "file:style-index.css"
}